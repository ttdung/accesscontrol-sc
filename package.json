{
  "name": "contracts",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "prepare": "husky install",
    "compile": "hardhat compile --show-stack-traces",
    "clean": "hardhat clean",
    "contract-size": "hardhat size-contracts --no-compile",
    "function-selectors": "hardhat finder --prettify --colorify method-identifiers --no-compile",
    "test": "hardhat test --network hardhat",
    "coverage": "env COVERAGE=true hardhat coverage",
    "solhint": "prettier --loglevel warn --ignore-path .gitignore '{contracts,test}/**/*.sol' --check && solhint -f table 'contracts/**/*.sol'",
    "lint:fix": "prettier --write 'contracts/**/*.sol'",
    "slither": "npm run clean && slither .",
    "gen-go": "hardhat export-abi && ./gen-go-package.sh",
    "deploy": "hardhat run scripts/deploy.ts --network devnet",
    "setDO": "hardhat run scripts/setDataOwner.ts --network devnet",
    "addFile": "hardhat addfiles --network devnet",
    "updateFile": "hardhat run scripts/updateFile.ts --network devnet",
    "listener": "hardhat run scripts/listener.ts --network devnet",
    "verify": "hardhat verify"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/<repo>.git"
  },
  "author": "",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/<repo>/issues"
  },
  "homepage": "https://github.com/<repo>#readme",
  "devDependencies": {
    "@commitlint/cli": "^17.4.4",
    "@commitlint/config-conventional": "^17.4.4",
    "@dlsl/hardhat-gobind": "^1.0.2",
    "@nomicfoundation/hardhat-chai-matchers": "^1.0.6",
    "@nomicfoundation/hardhat-network-helpers": "^1.0.8",
    "@nomiclabs/hardhat-ethers": "^2.2.2",
    "@nomiclabs/hardhat-etherscan": "^3.1.6",
    "@openzeppelin/hardhat-upgrades": "^1.22.1",
    "@typechain/ethers-v5": "^10.2.0",
    "@typechain/hardhat": "^6.1.5",
    "@types/chai": "^4.3.4",
    "@types/mocha": "^10.0.1",
    "@types/node": "^18.14.1",
    "@typescript-eslint/eslint-plugin": "^4.33.0",
    "@typescript-eslint/parser": "^4.33.0",
    "chai": "^4.3.7",
    "dotenv": "^10.0.0",
    "eslint": "^7.32.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-config-standard": "^16.0.3",
    "eslint-plugin-import": "^2.25.4",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-prettier": "^3.4.1",
    "eslint-plugin-promise": "^5.2.0",
    "ethereum-waffle": "^4.0.10",
    "hardhat": "^2.22.0",
    "hardhat-abi-exporter": "^2.10.1",
    "hardhat-contract-sizer": "^2.8.0",
    "hardhat-finder": "^3.0.0",
    "hardhat-gas-reporter": "^1.0.9",
    "husky": "^8.0.3",
    "matchstick-as": "^0.5.2",
    "mocha": "^10.2.0",
    "prettier": "^2.8.4",
    "prettier-plugin-solidity": "^1.1.2",
    "solhint": "^3.4.0",
    "solhint-plugin-prettier": "^0.0.5",
    "solidity-coverage": "^0.8.2",
    "ts-node": "^10.9.1",
    "typechain": "^8.1.1",
    "typescript": "^4.9.5"
  },
  "dependencies": {
    "@openzeppelin/contracts": "4.8.1",
    "@openzeppelin/contracts-upgradeable": "4.8.1",
    "request": "^2.88.2"
  }
}